module control_unit(in, immSel, regRW, ALUsrc, ALUop, MRW, WB, PCsrc);
	input [31:0] in;
	output reg regRW;
	output reg ALUsrc;
	output reg MRW;
	output reg WB;
	output reg PCsrc;
	output reg [1:0] immSel;
	output reg [3:0] ALUop;
	
